ers');\n    if (favoriteMovies) {\n      return favoriteMovies\n    }\n    if (favoriteCharacters) {\n      return favoriteCharacters\n    }\n  }\n\n  setLocalStorage() {\n    localStorage.setItem('favoriteMovies', this.state.favoriteMovies)\n    localStorage.setItem('favoritesCharacters', this.state.favoriteCharacters)\n\n  }\n\n  addMovieToFavorites(movie) {\n    // console.log('state', this.state)\n    this.setState({ \n      favoriteMovies: [...this.state.favoriteMovies, movie] \n    })\n  }\n\n  // removeMovieFromFavorites(arrayOfFav, name) {\n  //   let filteredDeletion = arrayOfFav.filter(fav => fav.title !== name)\n  //   this.setState({\n  //     favoriteMovies: filteredDeletion \n  //   })\n  // }\n\n  addCharToFavorites(char) {\n    // console.log('state', this.state)\n    this.setState(\n      { favoriteCharacters: [...this.state.favoriteCharacters, char] },\n    )\n  }\n\n  render() {\n    return (\n      <>\n        <Navigation favoriteMovies={this.state.favoriteMovies} favoriteCharacters={this.state.favoriteCharacters} />\n        <Route exact path='/' component={MovieList} />\n        <Route exact path='/films' component={MovieList} />\n        <Route path='/people/:charID' render={(props) => <CharacterDetails \n                                                          {...props} \n                                                          addCharToFavorites={this.addCharToFavorites} \n                                                          // removeMovieFromFavorites={this.removeMovieFromFavorites} \n                                                          currentFavorites={this.state.favoriteCharacters.map(char => char.name)}/>} />\n        <Route path='/films/:filmID' render={(props) => <MovieDetails {...props} addMovieToFavorites={this.addMovieToFavorites} currentFavorites={this.state.favoriteMovies.map(movie => movie.title)}/>} />\n        <Route path='/films/:filmID/characters' component={CharactersList} />\n      </>\n    )\n  }\n}\n\nexport default App;\n","/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/reportWebVitals.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/movies/index.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/movies/MovieTitle.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/util/index.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/util/getID.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/movies/MovieDetails.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/characters/index.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/characters/CharactersList.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/characters/CharacterDetails.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/movies/MoviesList.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/util/populateWithURLS.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/characters/CharFilmList.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/util/render.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/navigation/Navigation.js",["87"],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/navigation/index.js",[],"/Users/laurence.rega/Documents/Laurence/testreact/star-wars-movies-app-with-routes/src/components/navigation/FavMovies.js",[],"/Users/laurence.rega/Documents/Laurence/t